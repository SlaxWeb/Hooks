{
    "name": "slaxweb/hooks",
    "description": "Hooks library for SlaxWeb Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomaz Lovrec",
            "email": "tomaz.lovrec@gmail.com"
        }
    ],
    "require": {
        "php":  ">=7.0.0",
        "slaxweb/logger": "~0.3",
        "slaxweb/getset": "~0.1"
    },
    "require-dev": {
        "codeception/codeception": "dev-master",
        "behat/gherkin": "~4.4"
    },
    "suggest": {
        "pimple/pimple": "Make use of available providers for Pimple version 3.0.x"
    },
    "autoload": {
        "psr-4": {
            "SlaxWeb\\Hooks\\": "src/",
            "SlaxWeb\\Hooks\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.6.x-dev"
        }
    }
}
